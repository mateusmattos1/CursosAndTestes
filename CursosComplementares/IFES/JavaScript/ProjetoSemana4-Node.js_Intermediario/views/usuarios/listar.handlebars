<h1 class="text-center my-3">Lista de usuários</h1>
<table class="table">
 <div class="row">
 <div class="col-12">
 <thead>
 <th>Código do usuário</th>
 <th>Nome</th>
 <th>Email</th>
 </thead>
 <tbody>
 {{#each usuario}}
 <tr>
 <th>{{this.id_usuario}}</th>
 <th>{{this.nome}}</th>
 <th>{{this.email}}</th>
 </tr>
 {{/each}}
 </tbody>
 </div>
 </div>
</table>